CREATE TABLE "public"."Employee"
  ALTER COLUMN "email" TYPE varchar(255),
  ALTER COLUMN "phone_number" TYPE varchar(255),
  ALTER COLUMN "job_id" TYPE integer,
  ALTER COLUMN "salary" TYPE numeric,
  ALTER COLUMN "manager_id" TYPE integer,
  ALTER COLUMN "department_id" TYPE integer;
  ALTER COLUMN "hire_date" TYPE date,
  USING hire_date::date;
